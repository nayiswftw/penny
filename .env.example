# ── Gemini API ──────────────────────────────────────────────────
GEMINI_API_KEY=your_google_gemini_api_key_here
GEMINI_MODEL=gemini-2.5-pro
GEMINI_MAX_TOKENS=8192
GEMINI_TEMPERATURE=0.3

# ── Application ─────────────────────────────────────────────────
APP_TITLE=AI Financial Advisor
APP_ENV=development

# ── Data ────────────────────────────────────────────────────────
DATA_SOURCE=user_input
ALPHA_VANTAGE_KEY=

# ── Performance ─────────────────────────────────────────────────
LOG_LEVEL=INFO
CACHE_TTL_SECONDS=300
